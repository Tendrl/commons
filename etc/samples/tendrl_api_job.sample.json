# Each nvr of sds/node_agent rpm have their own unique tendrl definitions yaml,
# "sds_nvr"|"node_agent_nvr" used in tendrl api job body is the nvr of
# the sds/node_agent



# sds api job sample

{
	"cluster_id": "bc35fde8-528b-4908-8d52-50d8bf964dd4",
	"sds_name": "gluster",
	"sds_version": "3.2.0",
	"flow": "CreateGlusterVolume",
	"status": "processing",
	"parameters": {
		"volname": "Volume One"
	}
}

# node agent api job sample
# if job is intended for a specific node_agent, use "node_uuid"


{
	"cluster_id": "bc35fde8-528b-4908-8d52-50d8bf964dd4",
	"node_uuid": "node_uuid", 
	"flow": "ImportGlusterCluster",
	"status": "processing",
	"parameters": {
		"volname": "Volume One"
	}
}
